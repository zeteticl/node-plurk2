  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/form-data": "^2.5.2",
    "typedoc": "^0.21.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/node": "^10.17.26",
    "axios": "^1.10.0",
    "bluebird": "^3.7.2",
